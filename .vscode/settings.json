{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "explorerExclude.backup": null,
    "workbench.colorCustomizations": {
        "statusBar.foreground": "#F7F3FF",
        "statusBar.background": "#6A41B5",
        "statusBarItem.hoverBackground": "#5E2D74",
        "statusBarItem.remoteForeground": "#FFE7FD",
        "statusBarItem.remoteBackground": "#9A3091",
        "statusBar.debuggingForeground": "#170044",
        "statusBar.debuggingBackground": "#A078EC"
    },
    "jshint.enable": false,
    "standard.enable": false
}