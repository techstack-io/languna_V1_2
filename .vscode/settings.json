{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "explorerExclude.backup": null,
    "workbench.colorCustomizations": {
        "activityBar.foreground": "#580063",
        "activityBar.background": "#FDEDFF",
        "activityBar.inactiveForeground": "#C385FB",
        "activityBarBadge.foreground": "#FFF5F9",
        "activityBarBadge.background": "#FF7AAB",
        "statusBar.foreground": "#94FFEF",
        "statusBar.background": "#107D6D",
        "statusBarItem.hoverBackground": "#1D5163",
        "statusBarItem.remoteForeground": "#DAE9FF",
        "statusBarItem.remoteBackground": "#3261A7",
        "statusBar.debuggingForeground": "#000000",
        "statusBar.debuggingBackground": "#46B4A3"
    },
    "jshint.enable": false,
    "standard.enable": false
}