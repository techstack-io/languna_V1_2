{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "explorerExclude.backup": null,
    "workbench.colorCustomizations": {
        "statusBar.foreground": "#A1FFB0",
        "statusBar.background": "#18862A",
        "statusBarItem.hoverBackground": "#155D3A",
        "statusBarItem.remoteForeground": "#93FFF2",
        "statusBarItem.remoteBackground": "#107C6F",
        "statusBar.debuggingForeground": "#000000",
        "statusBar.debuggingBackground": "#4EBC61"
    },
    "jshint.enable": false,
    "standard.enable": false
}