{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "explorerExclude.backup": null,
    "workbench.colorCustomizations": {
        "statusBar.foreground": "#FAF3FF",
        "statusBar.background": "#7C39AF",
        "statusBarItem.hoverBackground": "#672A6B",
        "statusBarItem.remoteForeground": "#FFE2F6",
        "statusBarItem.remoteBackground": "#A32F7D",
        "statusBar.debuggingForeground": "#22003D",
        "statusBar.debuggingBackground": "#B270E6"
    },
    "jshint.enable": false,
    "standard.enable": false
}